@model IEnumerable<cho500.Models.ConsultReportListViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table table-striped table-responsive table-hover table-condensed">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FullName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HouseHoldNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Barangay)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateOfConsult)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AverageBP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RespiratoryRate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Temperature)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BMIStatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CheifComplaint)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.History)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Diagnostic)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Treatment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Recommendation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pharmacy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Physician)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HouseHoldNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Barangay)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateOfConsult)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AverageBP)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RespiratoryRate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Temperature)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BMIStatus)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CheifComplaint)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.History)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Diagnostic)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Treatment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Recommendation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pharmacy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Physician)
        </td>
    </tr>
}

</table>
