@model IEnumerable<cho500.Entity.HouseholdMember>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.HouseholdProfileID)</th>
        <th>@Html.DisplayNameFor(model => model.Member.FullName)</th>
        <th>@Html.DisplayNameFor(model => model.RelationToHead)</th>
        <th>@Html.DisplayNameFor(model => model.Member.PhilHealthNo)</th>
        <th>@Html.DisplayNameFor(model => model.Member.BloodType.Type)</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        @*<td>@Html.DisplayFor(modelItem => item.HouseholdProfileID)</td>*@
        <td>@Html.ActionLink(item.HouseholdProfileID,"Details","HouseholdProfiles", new { householdProfileID=item.HouseholdProfileID },null)</td>
        <td>@Html.DisplayFor(modelItem => item.Member.FullName)</td>
        <td>@Html.DisplayFor(modelItem => item.RelationToHead)</td>
        <th>@Html.DisplayFor(modelItem => item.Member.PhilHealthNo)</th>
        <th>@Html.DisplayFor(modelItem => item.Member.BloodType.Type)</th>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
